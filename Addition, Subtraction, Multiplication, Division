#include <stdio.h>

int main(){
    int x, y, sum, sub, multi, rem;
    printf("Enter the value of x = ");
    scanf("%d", &x);
    
    printf("Enter the value of y = ");
    scanf("%d", &y);
    
    sum = x+y;
    sub = x-y;
    multi = x*y;
    rem = x%y;

    printf("Addition = %d \nSubtraction = %d \nMultiplication = %d \nRemainder = %d \n", sum, sub, multi, rem);
    return 0;
}
