#include <stdio.h>

int main() {
    int n, first = 0, second = 1, next;

    // Input the number of terms you want in the Fibonacci sequence
    printf("Enter the number of terms: ");
    scanf("%d", &n);

    // Print the Fibonacci sequence
    printf("Fibonacci Sequence: \n");

    // Loop to generate the Fibonacci sequence
    for (int i = 0; i < n; i++) {
        if (i == 0) {
            printf("%d ", first);
            continue;
        }
        if (i == 1) {
            printf("%d ", second);
            continue;
        }
        next = first + second;
        printf("%d ", next);
        first = second;
        second = next;
    }

    printf("\n");
    return 0;
}
